<h1>Product Has Sales</h1>

{{! READ table }}
<table>
    <thead>
        {{! For the table header row, we print the DB attribute names }}
        <tr>
            <th>Product ID</th>
            <th>Sale ID</th>
            <th>Quantity</th>
            <th>Price</th>
        </tr>
    </thead>

    <tbody>
        {{! For each row, print the customerName, email, phoneNumber, and accountValue }}
        {{#each producthassales}}
        <tr>
            <td>{{this.productID}}</td>
            <td>{{this.saleID}}</td>
            <td>{{this.quantity}}</td>
            <td>{{this.price}}</td>
    </tbody>
    {{/each}}
</table>